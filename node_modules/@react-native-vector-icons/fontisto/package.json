{
  "name": "@react-native-vector-icons/fontisto",
  "version": "3.0.4-alpha.9",
  "description": "Fontisto font for react native vector icons",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "glyphmaps",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build ios/build lib",
    "prepare": "bob build",
    "watch": "onchange 'src/**' --initial -- yarn run prepare"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "osx",
    "windows",
    "macos",
    "react-component",
    "react-native-component",
    "react",
    "mobile",
    "ui",
    "icon",
    "icons",
    "vector",
    "retina",
    "font",
    "react-native-vector-icons-icon",
    "fontisto"
  ],
  "repository": {
    "url": "https://github.com/oblador/react-native-vector-icons",
    "type": "git",
    "directory": "packages/fontisto"
  },
  "author": {
    "name": "Joel Arvidsson",
    "email": "joel@oblador.se"
  },
  "license": "MIT",
  "homepage": "https://github.com/oblador/react-native-vector-icons",
  "bugs": {
    "url": "https://github.com/oblador/react-native-vector-icons/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@react-native-vector-icons/common": "^0.0.1-alpha.6"
  },
  "devDependencies": {
    "del-cli": "^5.1.0",
    "fontisto": "3.0.4",
    "onchange": "^7.1.0",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  },
  "gitHead": "f0916fe4918c204caf641f094d6e2367342def87"
}
